  doi           = {10.1007/978-3-319-67089-8_2},
  editor        = {Hague, Matthew and Potapov, Igor},
  isbn          = {978-3-319-67089-8},
  keywords      = {timed automata and pushdown automata and citesme!},
  pages         = {9--25},
  publisher     = {Springer},
  title         = {Adding Dense-Timed Stack to Integer Reset Timed Automata},
