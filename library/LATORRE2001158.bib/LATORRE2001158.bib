  issn          = {1571-0661},
  journal       = {Electronic Notes in Theoretical Computer Science},
  note          = {Computing: The Australasian Theory Symposium (CATS 2001)},
  pages         = {158 - 173},
  title         = {A Model of Finite Automata on Timed ω-Trees},
  url           = {http://www.sciencedirect.com/science/article/pii/S1571066104808843},
  volume        = {42},
  year          = {2001},
