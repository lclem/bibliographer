  issn          = {1571-0661},
  journal       = {Electronic Notes in Theoretical Computer Science},
  keywords      = {Parity tree automata, Mostowski index, decidability},
  note          = {Proceedings of the 11th Workshop on Logic, Language, Information and Computation (WoLLIC 2004)},
  pages         = {195--208},
  title         = {Deciding Nondeterministic Hierarchy of Deterministic Tree Automata},
