
@Article{         niwinskiwalukiewicz:entcs:2005,
  abstract      = {We show an algorithm which, for a given deterministic parity automaton on infinite trees, computes the minimal Mostowski (or Rabin) index of a nondeterministic automaton recognizing the same language. This extends a previous result of Urba{\'n}ski on deciding if a given deterministic Rabin automaton is equivalent to a nondeterministic B{\"u}chi automaton. The algorithm runs in the time of verifying the non-emptiness of nondeterministic parity automata.},
  author        = {Damian Niwi{\'n}ski and Igor Walukiewicz},
