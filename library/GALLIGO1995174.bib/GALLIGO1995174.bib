
@Article{         galligo1995174,
  abstract      = {Let W ⊂ Rn be a semialgebraic set defined by a quantifier-free formula with k atomic polynomials of the kind f ∈ Z[X1, . . . , Xn] such that degX1, . . . , Xn(f) < d and the absolute values of coefficients of f are less than 2M for some positive integers d, M. An algorithm is proposed for producing the complexification, Zariski closure, and also for finding all irreducible components of W. The running time of the algorithm is bounded from above by MO(1)(kd)nO(1). The procedure is applied to computing a Whitney system for a semialgebraic set and the real radical of a polynomial ideal.},
  author        = {A. Galligo and N. Vorobjov},
  date-added    = {2023-02-05 07:38:16 +0100},
  date-modified = {2023-02-05 07:38:16 +0100},
  doi           = {https://doi.org/10.1006/jcom.1995.1007},
  issn          = {0885-064X},
  journal       = {Journal of Complexity},
  number        = {1},
  pages         = {174-193},
  title         = {Complexity of Finding Irreducible Components of a Semialgebraic Set},
  url           = {https://www.sciencedirect.com/science/article/pii/S0885064X85710072},
  volume        = {11},
  year          = {1995},
