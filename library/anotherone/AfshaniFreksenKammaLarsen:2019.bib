
@Article{         afshanifreksenkammalarsen:2019,
  abstract      = {Multiplication is one of the most fundamental computational problems, yet its true complexity remains elusive. The best known upper bound, by F{\"u}rer, shows that two n n -bit numbers can be multiplied via a boolean circuit of size O ( n lg n ⋅ 4 lg ∗ n ) O(n \lg n \cdot 4^{\lg^*n}) , where lg ∗ n \lg^*n is the very slowly growing iterated logarithm. In this work, we prove that if a central conjecture in the area of network coding is true, then any constant degree boolean circuit for multiplication must have size Ω ( n lg n ) \Omega(n \lg n) , thus almost completely settling the complexity of multiplication circuits. We additionally revisit classic conjectures in circuit complexity, due to Valiant, and show that the network coding conjecture also implies one of Valiant's conjectures.},
  author        = {Peyman Afshani and Casper Benjamin Freksen and Lior Kamma and Kasper Green Larsen},
  date-added    = {2019-04-13 07:58:33 +0200},
  date-modified = {2019-04-13 07:58:33 +0200},
  eprint        = {1902.10935},
  month         = {02},
  title         = {Lower Bounds for Multiplication via Network Coding},
  url           = {https://arxiv.org/pdf/1902.10935},
  year          = {2019},
  bdsk-file-1   = {},
  bdsk-url-1    = {https://arxiv.org/pdf/1902.10935},
