  journal       = {Archive for History of Exact Sciences},
  number        = {5},
  pages         = {409--481},
  title         = {`Nobody could possibly misunderstand what a group is': a study in early twentieth-century group axiomatics},
  url           = {https://doi.org/10.1007/s00407-017-0193-8},
