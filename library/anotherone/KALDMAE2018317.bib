  issn          = {0005-1098},
  journal       = {Automatica},
  keywords      = {Realization theory, Time-delay systems, Nonlinear control systems, Algebraic approaches, Integrability},
  pages         = {317 - 320},
  title         = {Realization of time-delay systems},
  url           = {http://www.sciencedirect.com/science/article/pii/S0005109818300013},
  volume        = {90},
