  issn          = {0168-0072},
  journal       = {Annals of Pure and Applied Logic},
  pages         = {23 - 41},
  title         = {On G{\"o}del incompleteness and finite combinatorics},
  url           = {http://www.sciencedirect.com/science/article/pii/0168007287900741},
