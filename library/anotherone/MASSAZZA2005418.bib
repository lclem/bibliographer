
@Article{         massazza2005418,
  abstract      = {In this work, we study the problem of computing the coefficients of holonomic formal series in two commuting variables. Given a formal series φ(x,y)=∑n,k⩾0cnkxnyk specified by a holonomic system ∑j=0d1pj(x,y)∂xjφ=0 and ∑j=0d2qj(x,y)∂yjφ=0, with a suitable finite set of initial conditions {[xayb]φ(x,y)}, we show that the coefficient [xiyj]φ(x,y) can be computed in time O(i+j) under the uniform cost criterion.},
  author        = {P. Massazza and R. Radicioni},
  date-added    = {2020-10-02 19:08:56 +0200},
  date-modified = {2020-10-02 19:08:56 +0200},
  doi           = {https://doi.org/10.1016/j.tcs.2005.08.011},
  issn          = {0304-3975},
  journal       = {Theoretical Computer Science},
  keywords      = {Formal series, Coefficient problem, Holonomic systems},
  note          = {In memoriam: Alberto Del Lungo (1965-2003)},
  number        = {2},
  pages         = {418 - 438},
  title         = {On computing the coefficients of bivariate holonomic formal series},
  url           = {http://www.sciencedirect.com/science/article/pii/S0304397505004998},
  volume        = {346},
  year          = {2005},
