  issn          = {0168-0072},
  journal       = {Annals of Pure and Applied Logic},
  keywords      = {classic and readme},
  number        = {2},
  pages         = {149 - 184},
  title         = {Infinite games played on finite graphs},
  url           = {http://www.sciencedirect.com/science/article/pii/016800729390036D},
