  journal       = {SIAM Journal on Computing},
  number        = {5},
  pages         = {1210--1240},
  title         = {Word Problems and Membership Problems on Compressed Words},
  url           = {https://doi.org/10.1137/S0097539704445950},
  volume        = {35},
