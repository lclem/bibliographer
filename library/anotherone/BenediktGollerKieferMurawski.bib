
@Article{         benediktgollerkiefermurawski,
  abstract      = {Given two pushdown systems, the bisimilarity problem asks whether they are bisimilar. While this problem is known to be decidable our main result states that it is nonelementary, improving EXPTIME-hardness, which was the previously best known lower bound for this problem. Our lower bound result holds for normed pushdown systems as well.},
  author        = {Michael Benedikt and Stefan G{\"o}ller and Stefan Kiefer and Andrzej S. Murawski},
  date-added    = {2012-11-19 13:53:31 +0000},
  date-modified = {2012-11-19 13:53:31 +0000},
  eprint        = {1210.7686},
  month         = {10},
  title         = {Bisimilarity of Pushdown Systems is Nonelementary},
  url           = {http://arxiv.org/abs/1210.7686},
  year          = {2012},
  bdsk-file-1   = {},
  bdsk-url-1    = {http://arxiv.org/abs/1210.7686}
}
