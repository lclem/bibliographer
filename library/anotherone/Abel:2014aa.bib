
@Article{         abel:2014aa,
  abstract      = {In this paper, we present an Agda formalization of a normalizer for simply-typed lambda terms. The normalizer consists of two coinductively defined functions in the delay monad: One is a standard evaluator of lambda terms to closures, the other a type-directed reifier from values to eta-long beta-normal forms. Their composition, normalization-by-evaluation, is shown to be a total function a posteriori, using a standard logical-relations argument.},
  author        = {Andreas Abel and James Chapman},
  date-added    = {2017-05-03 19:59:35 +0000},
  date-modified = {2017-05-03 19:59:40 +0000},
  eprint        = {1406.2059},
  keywords      = {normalization by evaluation},
  month         = {06},
  title         = {Normalization by Evaluation in the Delay Monad: A Case Study for Coinduction via Copatterns and Sized Types},
  url           = {https://arxiv.org/abs/1406.2059},
  year          = {2014},
  bdsk-file-1   = {},
  bdsk-url-1    = {https://arxiv.org/abs/1406.2059}
