  issn          = {0304-3975},
  journal       = {Theoretical Computer Science},
  number        = {1},
  pages         = {215--231},
  title         = {A gap property of deterministic tree languages},
  url           = {http://www.sciencedirect.com/science/article/pii/S0304397502004528},
  volume        = {303},
  year          = {2003},
