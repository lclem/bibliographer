  doi           = {https://doi.org/10.1016/j.jsc.2008.02.010},
  issn          = {0747-7171},
  journal       = {Journal of Symbolic Computation},
  keywords      = {Characteristic set, Differential and difference polynomial, Regular ascending chain, Irreducible ascending chain, Zero decomposition algorithm, Perfect ideal membership problem},
  note          = {Effective Methods in Algebraic Geometry},
  number        = {9},
  pages         = {1137 - 1163},
  title         = {Characteristic set method for differential--difference polynomial systems},
