	issn = {0024-3795},
	journal = {Linear Algebra and its Applications},
	pages = {225-260},
	title = {Exponential numbers of linear operators in normed spaces},
	url = {https://www.sciencedirect.com/science/article/pii/002437959300217N},
	volume = {219},
	year = {1995},
