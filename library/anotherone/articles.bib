	editor = {Sankaranarayanan, Sriram and Sharygina, Natasha},
	isbn = {978-3-031-30823-9},
	pages = {249--270},
	publisher = {Springer Nature Switzerland},
	title = {Modular Mix-and-Match Complementation of B{\"u}chi Automata},
