	issn = {1529-3785},
	issue_date = {January 2021},
	journal = {ACM Trans. Comput. Logic},
	keywords = {imperfect information, perfect recall, Strategic reasoning, distributed synthesis, Nash equilibria, hierarchical information, rational synthesis},
	month = jan,
	number = {1},
	numpages = {51},
	publisher = {Association for Computing Machinery},
	title = {Strategy Logic with Imperfect Information},
