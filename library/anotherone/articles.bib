	doi = {10.1145/2038642.2038659},
	isbn = {9781450307147},
	keywords = {invariant, semi-algebraic set, polynomial dynamical system},
	location = {Taipei, Taiwan},
	numpages = {10},
	pages = {97--106},
	publisher = {Association for Computing Machinery},
	series = {EMSOFT '11},
	title = {Computing Semi-Algebraic Invariants for Polynomial Dynamical Systems},
