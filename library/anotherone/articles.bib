	issn = {1999-4893},
	journal = {Algorithms},
	number = {3},
	title = {Determinization and Minimization of Automata for Nested Words Revisited},
	url = {https://www.mdpi.com/1999-4893/14/3/68},
	volume = {14},
	year = {2021},
