	issn = {0304-3975},
	journal = {Theoretical Computer Science},
	keywords = {Arithmetic circuits, Complexity classes, , Oracle hierarchy, Threshold classes, Exact counting classes},
	note = {Mathematical Foundations of Computer Science (MFCS 2010)},
	pages = {36--49},
	title = {Counting classes and the fine structure between NC1 and L},
	url = {https://www.sciencedirect.com/science/article/pii/S0304397511004737},
	volume = {417},
	year = {2012},
	bdsk-file-1 = {},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0304397511004737},
