	doi = {10.1145/3462673},
	issn = {1529-3785},
	issue_date = {October 2021},
	journal = {ACM Trans. Comput. Logic},
	keywords = {Abstract interpretation, regular language, context-free language, completeness, language inclusion, one-counter net, automaton, grammar},
	month = sep,
	number = {4},
	numpages = {40},
	publisher = {Association for Computing Machinery},
	title = {Complete Abstractions for Checking Language Inclusion},
