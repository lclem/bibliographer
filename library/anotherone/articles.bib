	editor = {Jobstmann, Barbara and Leino, K. Rustan M.},
	isbn = {978-3-662-49122-5},
	pages = {268--288},
	publisher = {Springer Berlin Heidelberg},
	title = {A Method for Invariant Generation for Polynomial Continuous Systems},
	year = {2016},
	bdsk-file-1 = {},
