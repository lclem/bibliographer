	journal = {Computers & Mathematics with Applications},
	keywords = {Algorithms, Alternation, Complexity theory, Game theory, Incomplete information, Blindfold},
	number = {1},
	pages = {179-206},
	title = {Decision algorithms for multiplayer noncooperative games of incomplete information},
	url = {https://www.sciencedirect.com/science/article/pii/S0898122101002826},
