	journal = {Journal of Computer and System Sciences},
	number = {1},
	pages = {116--148},
	title = {Gap-definable counting classes},
	url = {https://www.sciencedirect.com/science/article/pii/S0022000005800248},
	volume = {48},
	year = {1994},
	bdsk-file-1 = {},
