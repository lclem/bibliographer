	doi = {10.1002/cpe.6043},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/cpe.6043},
	journal = {Concurrency and Computation: Practice and Experience},
	keywords = {hybrid architectures, multilevel parallelism, parity games, performance analysis, Zielonka algorithm},
	number = {n/a},
	pages = {e6043},
	title = {Toward a multilevel scalable parallel Zielonka's algorithm for solving parity games},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/cpe.6043},
	volume = {n/a},
