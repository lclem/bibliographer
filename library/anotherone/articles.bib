	doi = {10.1145/3417995},
	issn = {0004-5411},
	issue_date = {October 2020},
	journal = {J. ACM},
	keywords = {logic, deterministic automata, automata over infinite words, Linear temporal logic, nondeterministic automata},
	month = oct,
	number = {6},
	numpages = {61},
	publisher = {Association for Computing Machinery},
	title = {A Unified Translation of Linear Temporal Logic to ω-Automata},
