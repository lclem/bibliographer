	journal = {Automatica},
	keywords = {Symbolic models, Approximate bisimulation, Digital control systems, Incremental stability, Nonlinear systems},
	number = {10},
	pages = {2508-2516},
	title = {Approximately bisimilar symbolic models for nonlinear control systems},
