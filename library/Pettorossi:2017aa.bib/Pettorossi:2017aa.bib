
@Article{         pettorossi:2017aa,
  abstract      = {It is well-known that: (i) every context-free language over a singleton terminal alphabet is regular, and (ii) the class of languages that satisfy the Pumping Lemma is a proper super-class of the context-free languages. We show that any language in this superclass over a singleton terminal alphabet is regular. Our proof is based on a transformational approach and does not rely on Parikh's Theorem. Our result extends previously known results because there are languages that are not context-free, do satisfy the Pumping Lemma, and do not satisfy the hypotheses of Parikh's Theorem.},
  author        = {Alberto Pettorossi and Maurizio Proietti},
  date-added    = {2017-09-25 06:19:16 +0000},
  date-modified = {2017-09-25 06:19:16 +0000},
  eprint        = {1705.09695},
  month         = {05},
  title         = {Regularity of non context-free languages over a singleton terminal alphabet},
  url           = {https://arxiv.org/abs/1705.09695},
  year          = {2017},
  bdsk-file-1   = {},
  bdsk-url-1    = {https://arxiv.org/abs/1705.09695}
}
