
@Article{         polymath,
  abstract      = {For any $m \geq 1$, let $H_m$ denote the quantity $H_m := \liminf_{n \to \infty} (p_{n+m}-p_n)$, where $p_n$ denotes the $n^{\operatorname{th}}$ prime; thus for instance the twin prime conjecture is equivalent to the assertion that $H_1$ is equal to two. In a recent breakthrough paper of Zhang, a finite upper bound was obtained for the first time on $H_1$; more specifically, Zhang showed that $H_1 \leq 70000000$.},
  author        = {D.H.J. Polymath},
