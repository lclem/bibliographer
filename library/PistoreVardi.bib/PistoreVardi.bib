
@Article{         pistorevardi,
  abstract      = {Linear Temporal Logic (LTL) is widely used for defining conditions on the execution paths of dynamic systems. In the case of dynamic systems that allow for nondeterministic evolutions, one has to specify, along with an LTL formula f, which are the paths that are required to satisfy the formula. Two extreme cases are the universal interpretation A.f, which requires that the formula be satisfied for all execution paths, and the existential interpretation E.f, which requires that the formula be satisfied for some execution path.},
  author        = {M. Pistore and M. Y. Vardi},
  date-added    = {2012-05-22 11:11:16 +0200},
  date-modified = {2012-05-23 05:20:33 +0000},
  eprint        = {1109.6346v1},
  keywords      = {Banach-Mazur games},
  month         = {09},
  title         = {The Planning Spectrum - One, Two, Three, Infinity},
  url           = {http://arxiv.org/abs/1109.6346v1},
  year          = {2011},
  bdsk-file-1   = {},
  bdsk-url-1    = {http://arxiv.org/abs/1109.6346v1}
