  eprint        = {https://doi.org/10.1142/S0129054113400339},
  journal       = {International Journal of Foundations of Computer Science},
  number        = {07},
  pages         = {1099--1116},
  title         = {Unambiguous constrained automata},
  url           = {https://doi.org/10.1142/S0129054113400339},
  volume        = {24},
  year          = {2013},
