  journal       = {Journal of Mathematical Analysis and Applications},
  number        = {1},
  pages         = {130 - 147},
  title         = {The linear differential equation whose solutions are the products of solutions of two given differential equations},
  url           = {http://www.sciencedirect.com/science/article/pii/0022247X84902828},
