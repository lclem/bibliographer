  isbn          = {1432-0622},
  journal       = {Applicable Algebra in Engineering, Communication and Computing},
  number        = {3},
  pages         = {243--262},
  title         = {From implicit to recursive equations},
  url           = {https://doi.org/10.1007/s00200-018-0370-2},
