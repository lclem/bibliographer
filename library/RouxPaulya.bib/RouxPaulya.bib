
@Article{         rouxpaulya,
  abstract      = {We show that under some general conditions the finite memory determinacy of a class of two-player win/lose games played on finite graphs implies the existence of a Nash equilibrium built from finite memory strategies for the corresponding class of multi-player multi-outcome games. This generalizes a previous result by Brihaye, De Pril and Schewe. For most of our conditions we provide counterexamples showing that they cannot be dispensed with.},
  author        = {St{\'e}phane Le Roux and Arno Pauly},
  date-added    = {2016-07-16 17:44:48 +0000},
  date-modified = {2016-07-16 17:54:51 +0000},
  eprint        = {1607.03356},
  keywords      = {citesme!},
  month         = {07},
  title         = {Extending Finite Memory Determinacy to Multiplayer Games},
  url           = {http://arxiv.org/abs/1607.03356},
  year          = {2016},
  bdsk-file-1   = {},
  bdsk-url-1    = {http://arxiv.org/abs/1607.03356}
