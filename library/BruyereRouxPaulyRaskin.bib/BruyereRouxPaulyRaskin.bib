
@Article{         bruyererouxpaulyraskin,
  abstract      = {We study multi-player turn-based games played on a directed graph, where the number of players and vertices can be infinite. An outcome is assigned to every play of the game. Each player has a preference relation on the set of outcomes which allows him to compare plays. We focus on the recently introduced notion of weak subgame perfect equilibrium (weak SPE). This is a variant of the classical notion of SPE, where players who deviate can only use strategies deviating from their initial strategy in a finite number of histories. Having an SPE in a game implies having a weak SPE but the contrary is generally false.},
  author        = {V{\'e}ronique Bruy{\`e}re and St{\'e}phane Le Roux and Arno Pauly and Jean-Fran{\c c}ois Raskin},
  date-added    = {2016-12-10 10:39:49 +0000},
  date-modified = {2016-12-10 10:40:04 +0000},
  eprint        = {1612.01402},
  keywords      = {citesme!},
  month         = {12},
  title         = {On the existence of weak subgame perfect equilibria},
  url           = {https://arxiv.org/abs/1612.01402},
  year          = {2016},
  bdsk-file-1   = {},
  bdsk-url-1    = {https://arxiv.org/abs/1612.01402}
}
