  issn          = {0304-3975},
  journal       = {Theoretical Computer Science},
  keywords      = {Computational complexity, Completeness, Combinational circuits, Arithmetic circuits},
  number        = {1},
  pages         = {211 - 229},
  title         = {The complexity of membership problems for circuits over sets of integers},
  url           = {http://www.sciencedirect.com/science/article/pii/S0304397506005585},
